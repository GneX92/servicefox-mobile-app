{
  "cli": {
    "version": ">= 16.20.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_BACKEND_API_URL": "https://service.waterkotte.de:3000",
        "EXPO_PUBLIC_APP_VERSION": "0.0.1-alpha"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
